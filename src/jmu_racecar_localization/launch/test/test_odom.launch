    <arg name="cmd_timeout" default="0.5"/> 
    <arg name="rectify"     default="false" />
    <arg name="stereo_view" default="false" />
    
    <include file="$(find jmu_racecar_localization)/launch/include/rf2o_laser_odometry.launch">
